<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="portlet.service">
	<namespace>miEspacio</namespace>
	
	<!--<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager uuid="true"">-->
	<entity local-service="true" name="Factura" remote-service="true" uuid="true">
		<!-- PK fields -->
		<column name="facturaId" primary="true" type="long" />
		<column name="numero" type="String" />
		<column name="cliente" type="String" />
		<column name="importe" type="String" />
	</entity>
</service-builder>